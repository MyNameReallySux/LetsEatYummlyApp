<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                    android:color="#ffeeef8e" />
            <stroke
                    android:width="1dp"
                    android:color="#ffffffff" />
            <corners
                    android:radius="6dp" />
            <padding
                    android:left="5dp"
                    android:top="5dp"
                    android:right="5dp"
                    android:bottom="5dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                    android:startColor="#ffa6ef93"
                    android:endColor="#ff156909"
                    android:angle="270" />
            <stroke
                    android:width="1dp"
                    android:color="#ff215617" />
            <corners
                    android:radius="6dp" />
            <padding
                    android:left="5dp"
                    android:top="5dp"
                    android:right="5dp"
                    android:bottom="5dp" />
        </shape>
    </item>
</selector>